{
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": 2016,
        "sourceType": "module"
    },
    "rules": {
       
        "linebreak-style": [
            0,
            "error",
            "windows"
        ],
       
        "semi": [
            "error",
            "always"
        ]
    }
}